{
  "name": "modyo-widget-vue-3",
  "description": "Widget Template for Modyo with a configuration for VueJS 3",
  "private": false,
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luisfalconmx/modyo-widget-vue-3.git"
  },
  "keywords": [
    "typescript",
    "postcss",
    "tailwindcss",
    "vue3",
    "vite",
    "modyo"
  ],
  "author": "luisfalconmx <luisfalconmx@gmail.com>",
  "bugs": {
    "url": "https://github.com/luisfalconmx/modyo-widget-vue-3/issues"
  },
  "homepage": "https://github.com/luisfalconmx/modyo-widget-vue-3#readme",
  "scripts": {
